{
  "name": "base-reactjs",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/icons": "^2.0.10",
    "@chakra-ui/react": "^2.2.4",
    "@chakra-ui/theme": "^2.1.3",
    "@chakra-ui/theme-tools": "^2.0.5",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@reduxjs/toolkit": "^1.8.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.6",
    "@types/react-slick": "^0.23.10",
    "axios": "^0.27.2",
    "bignumber.js": "^9.1.0",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "env-cmd": "^10.1.0",
    "formik": "^2.2.9",
    "framer-motion": "^6",
    "i18next": "^21.8.14",
    "i18next-browser-languagedetector": "^6.1.4",
    "moment": "^2.29.4",
    "openpgp": "^5.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropdown": "^1.11.0",
    "react-i18next": "^11.18.1",
    "react-icons": "^4.4.0",
    "react-paginate": "^8.1.3",
    "react-redux": "^8.0.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "5.0.1",
    "react-select": "^5.4.0",
    "react-slick": "^0.29.0",
    "react-tooltip": "^4.2.21",
    "react-viewport-list": "^6.0.3",
    "redux-persist": "^6.0.0",
    "sass": "^1.53.0",
    "slick-carousel": "^1.8.1",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "postinstall": "husky install",
    "start": "cross-env NODE_ENV='production' env-cmd -f .env react-scripts start",
    "start:dev": "cross-env NODE_ENV='development' env-cmd -f .env.development react-scripts start",
    "build": "env-cmd -f .env react-scripts build",
    "build:dev": "env-cmd -f .env.development react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-select": "^5.0.1",
    "@types/uuid": "^8.3.4",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  }
}
